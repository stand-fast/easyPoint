/* pages/easyPointCar/bookTicket/bookTicket.wxss */
.header{
    width: 100%;
    height: 270rpx;
    background-color: #7ecef4;
}
.kong{
    width: 100%;
    height: 36rpx;
}
.swiper_images{
    width: 92%;
    height: 300rpx;
    margin: 0 auto;
    box-shadow: 4rpx 4rpx 4rpx 4rpx gainsboro;
    border-radius: 24rpx;
}
.slide-image{
    width: 100%;
    height: 300rpx;
    border-radius: 24rpx;
}
.contain{
    width: 100%;
    margin-top: 90rpx;
    overflow: hidden;
    overflow-y: scroll;
}
.nav_switch{
    width: 60%;
    height: 60rpx;
    display: flex;
    margin:0 auto;
    flex-direction: row;
    margin-top: 18rpx;
}
.on{
    color: #7ecef4!important;
    border-bottom: 2rpx solid #7ecef4;
}
.nav_text{
    width: 50%;
    height: 60rpx;
    margin:0 auto;
    text-align: center;
    font-size: 34rpx;
    color: #999;
}
.hr{
    width: 92%;
    border:2rpx solid #eee;
    margin:0 auto;
    background-color: #eee;
}
.fullcontainer{
    width: 84%;
    margin: 0 auto;
}
.first{
    margin-top: 24rpx!important;
}
.formContent{
    width: 100%;
    height: 70rpx;
    margin-top: 22rpx;
    display: flex;
    flex-direction: row;
}
.formContent input{
    width: 356rpx;
    font-size: 30rpx;
    height: 30rpx;
    color: #333;
    line-height: 30rpx;
    margin: 16rpx 0rpx 8rpx 22rpx;
}
.placehold,.accoplace{
    font-size: 28rpx;
    color: #999;
    margin-top: 4rpx;
}
.formContent image{
    margin-top: 28rpx;
    width: 32rpx;
    height: 32rpx;
}
.downImg image{
    margin-top: 40rpx!important;
    width: 50rpx;
    height: 22rpx;
}
.association image{
    margin-top: 20rpx!important;
    width: 50rpx;
    height: 22rpx;
}
.formTitle{
    width: 130rpx;
    height: 30rpx;
    line-height: 30rpx;
    margin: 30rpx 18rpx 10rpx 30rpx;
    font-size: 27rpx;
    color: #999;
}
.wang{
    width: 88rpx!important;
    margin: 30rpx 20rpx 20rpx 1rpx!important;
}
.circle1{
    width: 22rpx;
    height: 22rpx;
    border-radius: 0;
    border:1rpx solid #bbb;
    background-color: #fff;
    margin: 35rpx 12rpx 16rpx 30rpx;
}
.circle2{
    width: 22rpx;
    height: 22rpx;
    border-radius: 0;
    border:1rpx solid #56b4f6;
    background-color: #56b4f6;
    margin: 35rpx 12rpx 16rpx 30rpx;
}
.circle2 image{
    width: 28rpx;
    height: 28rpx;
    position: fixed;
    margin: -1rpx 0 1rpx -3rpx;
}
.heng{
    height: 2rpx;
    background-color: #eee;
}
.protocal{
    display: flex;
    flex-direction: row;
    margin-top: 18rpx;
    color:#56b4f6;
    font-size: 30rpx!important;
}
.square2 image{
    width: 28rpx;
    height: 28rpx;
    position: fixed;
    margin: -3rpx -2rpx;
}
.square1{
    width: 22rpx;
    height: 22rpx;
    border-radius: 50%;
    border:1rpx solid #bbb;
    background-color: #fff;
    margin:7rpx 10rpx 0rpx 30rpx;
}
.square2{
    width: 22rpx;
    height: 22rpx;
    border-radius: 50%;
    border:1rpx solid #56b4f6;
    background-color: #56b4f6;
    margin:7rpx 10rpx 0 30rpx;
}
.Text{
    color:#56b4f6;
    font-size: 24rpx;
    text-decoration: underline;
}
 .baosquare1{
    width: 22rpx;
    height: 22rpx;
    border-radius: 0;
    border:1rpx solid #bbb;
    background-color: #fff;
    margin: 8rpx 10rpx 0rpx 60rpx;
}
.insurance_text{
    display: flex;
    flex-direction: row;
}
.baosquare2 image{
    width: 28rpx;
    height: 28rpx;
    margin: 0rpx -14rpx;
    position: fixed; 
}
.baosquare2{
    width: 22rpx;
    height: 22rpx;
    border-radius: 0;
    border:1rpx solid #56b4f6;
    background-color: #56b4f6;
    margin: 8rpx 10rpx 0rpx 60rpx;
}
.traBottom{
    width: 100%;
    height: 110rpx;
    position: fixed;
    bottom: 0rpx;
    display: flex;
    flex-direction: row;
}
.contactBtn{
    width: 160rpx;
    height: 110rpx;
    background-color: #56b4f6;
    box-sizing: border-box;
}
.contactBtn button{
    width: 100%;
    height: 100%;
    background-color: #7ecef4;
}
.contactBtn image{
    width: 70rpx;
    height: 70rpx;
    margin-top: 20rpx;
}
.payMoney{
    width: 400rpx;
    height: 110rpx;
    text-align: center;
    border-top: 1rpx solid #eee;
}
.money_text{
    height: 50rpx;
    line-height: 50rpx;
    margin-top: 6rpx;
    font-size: 26rpx;
    color: #333;
    margin-left: -70rpx;
}
.money_text text{
    height: 40rpx;
    line-height: 40rpx;
    color: #333;
    font-size: 30rpx;
}
.submitBtn{
    width: 190rpx;
    height: 100rpx;
}
.submitBtn button{
    width: 100%;
    height: 110rpx;
    padding-top:10rpx; 
    color: #fff;
    font-size: 36rpx;
    background-color: #7ecef4;
    border-radius: 0rpx;
    border: none;
}
.submitBtn button::after{
    border: none;
}
.association{
    width: 84%;
    height: 80rpx;
    margin: 60rpx auto;
    display: flex;
    flex-direction: row;
    border-bottom: 2rpx solid #eee;
}
.assoTitle{
    width: 160rpx;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 26rpx;
    color: #999;
    margin: 10rpx 20rpx 0 20rpx;
}
.association input{
    width: 350rpx;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 30rpx;
    color: #333;
    margin-top: 5rpx;
}
.searchBtn{
    width: 84%;
    height: 80rpx;
    margin: 160rpx auto;
}
.searchBtn button{
    border-radius: 0rpx;
    background-color: #7ecef4;
    color: #fff;
    font-size: 30rpx;
}
.searchBtn button::after{
    border: none;
}
.mask_layer {
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 999;
    left:0;top:0;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}
.modal_box {
    width: 90%;
    height: 400rpx;
    overflow: hidden;
    position: fixed;
    top: 30%;
    left: 0;
    z-index: 1001;
    background: #fafafa;
    margin: 5%;
    border-radius: 20rpx;
}
.model_success{
    margin:60rpx 0 30rpx 0;
    text-align: center;
    font-size: 36rpx;
    color: #56b4f6;
}
.success_text{
    text-align: center;
    font-size: 26rpx;
    color: #999;
    height: 44rpx;
    line-height: 44rpx;
    padding: 0rpx 20%;
}
.success_btn{
    background-color: #56b4f6;
    color: #fff;
    font-size: 30rpx;
    margin-top: 88rpx;
    margin-bottom: 20rpx;
    width: 240rpx;
    border-radius: 60rpx;
    height: 70rpx;
    line-height: 70rpx;
}
.cnimg{
    margin-left: 8rpx;
}